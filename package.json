{
  "name": "network-helper",
  "version": "1.0.0",
  "description": "A tool for helping keep track of your personal and professional network",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.main.config.cjs && webpack --config webpack.renderer.config.cjs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmcilwain6/network-helper.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rmcilwain6/network-helper/issues"
  },
  "homepage": "https://github.com/rmcilwain6/network-helper#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "electron": "^38.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "html-webpack-plugin": "^5.6.4",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.9",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "better-sqlite3": "^12.3.0",
    "electron-rebuild": "^3.2.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
